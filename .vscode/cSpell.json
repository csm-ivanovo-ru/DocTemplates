{
    "version": "0.1",
    "ignorePaths": [
        ".git",
        ".git/**",
        ".vscode/*",
        "node_modules",
        "**/node_modules",
        "**/node_modules/**",
        "node_modules/**"
    ],
    "language": "ru,en",
    "dictionaries": [],
    "words": [
        "Документооборот",
        "расширения",
        "рефакторинг",
        "Штрихкод",
        "betke",
        "codecov",
        "commitlint",
        "markdownlint",
        "mimetype",
        "mkdocs",
        "opendocument",
        "Paren",
        "precommit",
        "preprocess",
        "PreRelease",
        "Recurse",
        "repo",
        "shellcheck",
        "Veyor",
        "xlink"
    ],
    "ignoreRegExpList": [
        "эскалаци(я|и|ю|ей)",
        "автоответ(а|у|ом|е|ы|ов|ам|ами|ах)",
        "автоподстанов(ка|ки|ке|кой|ку|ки|ок|кам|ках)",
        "автозаполнени(е|я|ем|и|ю|й|ям|ях)",
        "AppVeyor",
        "репозитори(й|я|ю|ем|и)",
        "\\.gitattributes",
        "\\[.*?\\]\\(.*?\\)"
    ],
    "enableFiletypes": [
        "xslt"
    ]
}