*.ttf  binary

*.xml  text  merge=text diff=text eol=lf
*.xsd  text  merge=text diff=text eol=lf
*.xsl  text  merge=text diff=text eol=lf

*.svg  text  merge=text diff=text eol=lf

*.md   text  merge=text diff=text eol=crlf

*.ps1  text  merge=text diff=text eol=crlf
*.psd1 text  merge=text diff=text eol=crlf
*.psm1 text  merge=text diff=text eol=crlf

*.bas  text  merge=text diff=text eol=lf

*.{xba,xdl,xlb,xlc}  text  merge=text diff=text eol=lf

*.json text  merge=json diff=text eol=crlf
